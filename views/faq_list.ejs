<%-include('./admin_header.ejs')%>
<%-include('./admin_left_menu_bar.ejs')%>


<section class="content dashboard">
    
   
    <div class="page-heading">
        <h1>Faq List</h1>
        <ol class="breadcrumb">
            <li><a href="/admin/index">Home</a></li>
            <li class="active">faq list</li>
        </ol>
    </div>
   
    <div class="panel panel-default">

        <div class="panel-body">
            <div class="table-responsive">
                <table id="myTable" class="table js-exportable table-bordered">

                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>Title</th>
                            <th>Description</th>
                            <th>Edit</th>  
                        </tr>
                    </thead>
                    <tbody>
                       <% for(var i=0; i < data.length; i++) { %>
                            <tr>
                                
                            <td><%= i+1%></td>
                            <td><%=data[i].title%></td> 
                            <td><textarea readonly><%=data[i].description%></textarea></td>
                            
                            
                    
                            <td>
                        
                               <a href="/admin/edit_faq/<%=data[i]._id%>" class="btn btn-primary">
                                <i class="fa fa-edit"></i>
                                </a>
                                 
                            </td>

                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    
    </div>
</section>
<%-include('./admin_footer.ejs')%>